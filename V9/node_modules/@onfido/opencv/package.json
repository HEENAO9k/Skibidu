{
  "name": "@onfido/opencv",
  "version": "2.1.1",
  "description": "Open CV package rebundled for Onfido usage",
  "main": "dist/opencv.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist/"
  ],
  "exports": {
    ".": {
      "import": "dist/opencv.js",
      "require": "dist/opencv.js",
      "types": "dist/types/index.d.ts"
    },
    "./wasm": {
      "import": "dist/opencv.wasm.js",
      "require": "dist/opencv.wasm.js",
      "types": "dist/types/index.d.ts"
    }
  },
  "scripts": {},
  "keywords": [
    "opencv",
    "javascript",
    "computer vision"
  ],
  "author": "Wilson",
  "license": "Apache-2.0",
  "dependencies": {
    "mirada": "^0.0.15"
  }
}
